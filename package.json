{
  "name": "wood-import-export-system",
  "version": "1.0.0",
  "description": "Rossiyadan yog'och import/export boshqaruv tizimi",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "server:start": "cd server && npm start",
    "client:start": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "migrate:vagon": "node scripts/migrate-vagon-fields.js",
    "test:business": "node scripts/test-real-business-logic.js"
  },
  "keywords": ["wood", "import", "export", "russia", "uzbekistan"],
  "author": "Wood Import/Export System",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}